{
  "name": "carga-safe",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "db:server": "json-server --watch server/db.json --routes server/routes.json",
    "test": "ng test",
    "local:db": "concurrently \"npm run db:server\" \"npm start\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.3",
    "@angular/cdk": "^20.2.7",
    "@angular/common": "^20.3.3",
    "@angular/compiler": "^20.3.3",
    "@angular/core": "^20.3.3",
    "@angular/forms": "^20.3.3",
    "@angular/google-maps": "^20.2.8",
    "@angular/material": "^20.2.7",
    "@angular/platform-browser": "^20.3.3",
    "@angular/platform-browser-dynamic": "^20.3.3",
    "@angular/router": "^20.3.3",
    "@swimlane/ngx-charts": "^23.0.1",
    "d3": "^7.9.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "jwt-decode": "^4.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.3",
    "@types/jasmine": "~5.1.0",
    "@types/jspdf": "^1.3.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "jasmine-core": "~5.9.0",
    "json-server": "^0.17.4",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "typescript": "~5.9.2"
  }
}
